  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jasmine - All Tests",
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": ["node_modules/jasmine/bin/jasmine", "--config=jasmine.json"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jasmine - Current File",
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": ["node_modules/jasmine/bin/jasmine", "--config=jasmine.json", "${file}"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
